    "angular-ui-bootstrap": "^2.5.0",
    "angucomplete-alt": "^3.0.0",
    "angular-bootstrap": "^2.5.0",
    "jquery-ui": "^1.12.1"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/fonts/*",
        "dist/js/bootstrap.min.js"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "fonts/*"
      ]
    },
    "slick-carousel": {
      "main": [
        "slick/*.css",
        "slick/fonts/*",
        "slick/*.min.js",
        "slick/*"
      ]
    },
    "jquery-ui": {
      "main": [
        "jquery-ui.min.js",
        "jquery-ui.js",
        "themes/*/*.*",
        "themes/*/*/*.*"
      ]
    }
  }
