    "tests"
  ],
  "dependencies": {
    "jquery": "^3.2.1",
    "slick-carousel": "^1.7.1",
    "font-awesome": "^4.7.0",
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4",
    "angular": "^1.6.6",
